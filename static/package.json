  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "modules": false,
          "corejs": 3
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleNameMapper": {
      "^@/(.+)": "<rootDir>/resource/base/$1"
    },
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/resource/tests/**/*.test.+(ts|js)"
    ]
  },
  "scripts": {
    "setup": "rm -rf .git",
    "dev": "gulp",
    "build": "gulp onBuild",
    "clean": "gulp onClean",
    "lint": "eslint 'resource/**/*.{ts,js}' --max-warnings 0",
    "lint:fix": "eslint --fix 'resource/**/*.{ts,js}' --max-warnings 0",
    "stylelint": "stylelint 'resource/**/*.scss'",
    "jest": "jest --passWithNoTests"
  }
